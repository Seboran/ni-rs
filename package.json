{
  "name": "@seboran/ni",
  "type": "module",
  "version": "0.2.9",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/Seboran/ni-rs"
  },
  "packageManager": "pnpm@^8.0.0",
  "files": [
    "npm/dist",
    "npm/bin"
  ],
  "bin": {
    "ni": "npm/bin/ni.cjs",
    "na": "npm/bin/na.cjs",
    "nr": "npm/bin/nr.cjs",
    "nrm": "npm/bin/nrm.cjs"
  },
  "scripts": {
    "compile": "cargo build --workspace --bins -r && cp target/release/na target/release/ni target/release/nr target/release/nrm npm/dist",
    "link:local": "cd npm && bun link"
  },
  "workspaces": [
    "npm"
  ],
  "devDependencies": {
    "@types/node": "^20.11.28"
  }
}
